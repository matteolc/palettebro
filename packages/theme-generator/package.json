{
  "name": "@palettebro/theme-generator",
  "version": "0.1.1",
  "private": false,
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./server": {
      "import": "./dist/services/index.mjs",
      "require": "./dist/services/index.js"
    },
    "./client": {
      "import": "./dist/palettes/index.mjs",
      "require": "./dist/palettes/index.js"
    },
    "./types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "format-and-lint": "biome check .",
    "format-and-lint:fix": "biome check . --write"
  },
  "dependencies": {
    "@langchain/community": "0.3.15",
    "@langchain/core": "0.3.15",
    "@langchain/openai": "0.3.15",
    "@material/material-color-utilities": "0.3.0",
    "apca-w3": "^0.1.9",
    "delta-e": "^0.0.8",
    "culori": "^4.0.1",
    "nearest-color": "^0.4.4",
    "node-fetch": "^2.6.9",
    "picocolors": "^1.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@palettebro/biome-config": "workspace:*",
    "@palettebro/typescript-config": "workspace:*",
    "@types/apca-w3": "^0.1.3",
    "@types/node-fetch": "^2.6.12",
    "@types/culori": "^2.1.1",
    "@types/delta-e": "^0.0.2",
    "tsup": "^8.3.5",
    "typescript": "5.6.3"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "@types/react": "^18.3.12"
  },
  "authors": [
    {
      "name": "Matteo La Cognata",
      "email": "matteo.lacognata@nearform.com",
      "url": "https://github.com/matteolc"
    }
  ],
  "license": "MIT",
  "files": ["dist"],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
